import java.util.Scanner;

class Main {
  public static void main(String[] args) {
  Scanner sc = new Scanner(System.in);
  System.out.println("digite uma palavra");
  String nome1 = sc.next();
  String nome2 = ("aula");
  nome1 = ("bula");       
  nome2 = ("rua");
  System.out.println(nome1+nome2);
  }
}
